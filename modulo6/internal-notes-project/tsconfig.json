{
  "compilerOptions":{
      "target":"es5",
      "outDir": "./build",      
      "rootDir": "./src",   
      "module":"commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false
  }
}